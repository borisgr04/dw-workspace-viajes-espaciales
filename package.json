{
  "name": "dw-workspace",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build:navbar": "ng build --project dw-navbar  --output-hashing none && node navbar-build.js",
    "build:pagination": "ng build --project dw-pagination  ",
    "build:toolbar": "ng build --project dw-ng-toolbar  --output-hashing none && node toolbar-build.js",
    "build:dw-ng-toolbar0": "ng build --project dw-ng-toolbar --output-hashing none && npm run pack:dw-ng-toolbar && cp projects/dw-ng-toolbar/package.json dist/dw-ng-toolbar",
    "pack:dw-ng-toolbar0": "cat ./dist/dw-ng-toolbar/{runtime,polyfills,main}-es5.js > dist/dw-ng-toolbar/dw-ng-toolbar.js && ls -lah dist/dw-ng-toolbar/dw-ng-toolbar.js",
    "build:dw-toolbar": "ng build --project dw-toolbar"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.2.0",
    "@angular/common": "~13.2.0",
    "@angular/compiler": "~13.2.0",
    "@angular/core": "~13.2.0",
    "@angular/elements": "^13.2.3",
    "@angular/forms": "~13.2.0",
    "@angular/localize": "~13.2.0",
    "@angular/platform-browser": "~13.2.0",
    "@angular/platform-browser-dynamic": "~13.2.0",
    "@angular/router": "~13.2.0",
    "@angular/service-worker": "~13.2.0",
    "@ng-bootstrap/ng-bootstrap": "^12.0.0",
    "@popperjs/core": "^2.10.2",
    "bootstrap": "^5.1.3",
    "dw-navbar": "^0.0.2",
    "dw-toolbar": "^0.0.2",
    "dw-toolbar-npm": "^0.0.1",
    "jw-paginate": "^1.0.4",
    "moment": "^2.29.1",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.2.4",
    "@angular/cli": "~13.2.2",
    "@angular/compiler-cli": "~13.2.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "angular-in-memory-web-api": "^0.13.0",
    "concat": "^1.0.3",
    "fs-extra": "^10.0.0",
    "husky": "^4.3.8",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "^13.0.0",
    "typescript": "~4.5.2"
  }
}
